{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ],
      "~~/*": [
        "./*"
      ],
      "@@/*": [
        "./*"
      ]
    },
    "jsx": "preserve",
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ],
  "vueCompilerOptions": {
    "extensions": [
      ".vue"
    ]
  }
}
